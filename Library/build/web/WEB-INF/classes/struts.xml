<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        
        <action name="test" class="userRegister" method="execute">
            <result name="success">/dashboard.jsp</result>
        </action>
        
        
        
        <action name="loginTest" class="LoginValidate" method="execute">
            <result name="success">/Home.jsp</result>
            <!--<result name="error">/LoginSuccess.jsp</result>-->
        </action>
        
        <action name="add" class="Books" method="execute">
            <result name="success">/Home.jsp</result>
        </action>
        
        <action name="display" class="BooksAction" method="execute">
                <result name="success">/List.jsp</result>
        </action>
        
    </package>
</struts>
